{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/include/nlohmann",
                "C:/MinGW/include/**",
                "C:/MinGW/include",
                "C:/MinGW/lib/gcc/mingw32/*/include/c++",
                "C:/MinGW/lib/gcc/mingw32/*/include/c++/mingw32",
                "C:/MinGW/lib/gcc/mingw32/*/include/c++/backward",
                "C:/MinGW/lib/gcc/mingw32/*/include",
                "C:/MinGW/mingw32/include",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/ucrt",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/um",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/shared",
                "C:/ScopePaymentApi/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "WIN32",
                "_WIN32_WINNT=0x0601",
                "__cplusplus=201703L",
                "WINVER=0x0601"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "C:/MinGW/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-std=c++17",
                "-pthread",
                "-Wall",
                "-Wextra",
                "-static-libgcc",
                "-static-libstdc++",
                "-mthreads"
            ],
            "configurationProvider": "ms-vscode.cpptools"
        }
    ],
    "version": 4
